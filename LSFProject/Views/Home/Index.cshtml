<section class="latest-news-area">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <h1 class="area-heading font-sky style-two">Последние новости</h1>
            </div>
        </div>
        <div class="row justify-content-center no-gutters">
            <div class="col-xl-8">
                <div class="row">
                    @foreach (News newsItem in _context.News.OrderByDescending(date => date.Date).Where(news => !news.Blocked).Take(3))
                    {
                        <div class="col-md-4 col-xl-4">
                            <div class="single-news wow wow flipInY" data-wow-delay=".7s">
                                <img src="@newsItem.PreviewPhoto" alt="">
                                <div class="news-det">
                                    <h4>@newsItem.Header</h4>
                                    <p>@newsItem.PreviewText</p>
                                    <div class="news-meta">
                                        <span>
                                            <i class="fa fa-comment"></i>
                                            @_context.Comments.Where(id => id.NewsId == newsItem.Id).ToList().Count() Comments
                                        </span><span>
                                            <i class="fa fa-share-alt-square"></i>
                                            @newsItem.Share Share
                                        </span>
                                        <a href=""><i class="fa fa-angle-right"></i></a>
                                    </div>
                                </div>
                            </div>

                        </div>
                    }
                </div>

            </div>
        </div>
    </div>
</section>
